<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.ps.geodroidapp.com.example.humbertosilva.geodroidapp.Activities.MainActivityStart"
    >

    <Button
        android:id="@+id/button_createSession"
        android:layout_width="202dp"
        android:layout_height="85dp"
        android:text="@string/mainActivityStart_createSession"
        android:textAllCaps="false"
        android:textSize="20sp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_existingSession" />

    <Button
        android:id="@+id/button_existingSession"
        android:layout_width="202dp"
        android:layout_height="85dp"
        android:text="@string/mainActivityStart_existingSession"
        android:textAllCaps="false"
        android:textSize="20sp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/button_createSession"
        app:layout_constraintTop_toTopOf="parent" />

</android.support.constraint.ConstraintLayout>
